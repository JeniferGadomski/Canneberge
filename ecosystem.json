{
  "apps" : [{
    "name"        : "API",
    "script"      : "./api.js",
    "watch"       : false,
    "cwd"	: "./API/"
  },
  {
  	"name"        : "Portail",
    "script"      : "/usr/bin/http-server",
    "watch"       : false,
    "cwd"	: "./Portail/",
    "args" : "-p 8002"
  },
  {
  	"name"        : "Carte",
    "script"      : "/usr/bin/http-server",
    "watch"       : false,
    "cwd"	: "./Carte/",
    "args" : "-p 3001"
  },
  {
  	"name"        : "Admin",
    "script"      : "/usr/bin/http-server",
    "watch"       : false,
    "cwd"	: "./Admin/dist/",
    "args" : "-p 8000"
  },
  {
  	"name"        : "R",
    "script"      : "/var/FastRWeb/code/start",
    "watch"       : false,
    "interpreter" : "bash",
    "args" : "-d",
    "out_file" : "/dev/null",
  },
  {
  	"name"        : "Documentation",
    "script"      : "/usr/bin/http-server",
    "watch"       : false,
    "cwd"	: "./Documentation/API/",
    "args" : "-p 8001"
  }
  ]
}
